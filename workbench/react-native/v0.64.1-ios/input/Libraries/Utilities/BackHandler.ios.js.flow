// @flow
type BackPressEventName = 'backPress' | 'hardwareBackPress';

type TBackHandler = {|
  +exitApp: () => void,
  +addEventListener: (
    eventName: BackPressEventName,
    handler: () => ?boolean
  ) => {remove: () => void,...},
  +removeEventListener: (
    eventName: BackPressEventName,
    handler: () => ?boolean
  ) => void,
|};

declare var BackHandler: TBackHandler;
declare module.exports: typeof BackHandler;
