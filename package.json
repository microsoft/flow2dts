{
  "private": true,
  "name": "flow2dts-monorepo",
  "workspaces": [
    "packages/*",
    "workbench/react-native/v0.63.3",
    "workbench/artsy-eigen"
  ],
  "scripts": {
    "test": "yarn workspace flow2dts test",
    "start": "yarn workbench && yarn workbench:check:offences",
    "workbench": "yarn workbench:react-native:v0.63.3",
    "workbench:check": "yarn workbench:react-native:v0.63.3:check",
    "workbench:check:offences": "yarn workbench:check | node ./scripts/tsc-offences.js",
    "workbench:react-native:v0.63.3": "yarn workbench:react-native:v0.63.3:ios && yarn workbench:react-native:v0.63.3:android",
    "workbench:react-native:v0.63.3:check": "yarn workbench:react-native:v0.63.3:check:ios && yarn workbench:react-native:v0.63.3:check:android",
    "workbench:react-native:v0.63.3:cli": "yarn workspace flow2dts cli --cwd ../.. --rootDir ./workbench/react-native/v0.63.3/inputs --overrides ./workbench/react-native/v0.63.3/overrides.ts --hint ./workbench/react-native/v0.63.3/hint/hint.json",
    "workbench:react-native:v0.63.3:cli-with-globs": "yarn workbench:react-native:v0.63.3:cli \"./workbench/react-native/v0.63.3/inputs/{index,Libraries/**/*}.js.flow\" \"!./workbench/react-native/v0.63.3/inputs/Libraries/**/{__flowtests__,__mocks__,__tests__}/**/*.js.flow\" \"!./workbench/react-native/v0.63.3/inputs/Libraries/{Inspector,JSInspector,NewAppScreen,polyfills,ReactPrivate}/**/*.js.flow\" \"!./workbench/react-native/v0.63.3/inputs/Libraries/Animated/AnimatedWeb.js.flow\" \"!./workbench/react-native/v0.63.3/inputs/Libraries/{Promise,promiseRejectionIsError,promiseRejectionTrackingOptions}.js.flow\" \"!./workbench/react-native/v0.63.3/inputs/Libraries/vendor/core/ErrorUtils.js.flow\"",
    "workbench:react-native:v0.63.3:ios": "rimraf ./workbench/react-native/v0.63.3/outputs/ios/Libraries && yarn workbench:react-native:v0.63.3:cli-with-globs --platform ios --overrides ./workbench/react-native/v0.63.3/overrides.ios.ts --outDir ./workbench/react-native/v0.63.3/outputs/ios",
    "workbench:react-native:v0.63.3:android": "rimraf ./workbench/react-native/v0.63.3/outputs/android/Libraries && yarn workbench:react-native:v0.63.3:cli-with-globs --platform android --overrides ./workbench/react-native/v0.63.3/overrides.android.ts --outDir ./workbench/react-native/v0.63.3/outputs/android",
    "workbench:react-native:v0.63.3:check:ios": "yarn tsc --project ./workbench/react-native/v0.63.3/outputs/ios/tsconfig.json --pretty",
    "workbench:react-native:v0.63.3:check:android": "yarn tsc --project ./workbench/react-native/v0.63.3/outputs/android/tsconfig.json --pretty",
    "workbench:react-native:v0.63.3:ios:npm": "node scripts/create-types-package.js ./workbench/react-native/v0.63.3/package.json ./workbench/react-native/v0.63.3/outputs/ios",
    "workbench:artsy-eigen:check": "yarn workspace artsy-eigen type-check",
    "workbench:artsy-eigen:check:offences": "yarn workspace artsy-eigen type-check | node ./scripts/tsc-offences.js",
    "hint": "yarn hint:react-native:ios",
    "hint:lazy": "yarn hint:react-native:ios:lazy",
    "hint:react-native:ios": "rimraf ./workbench/react-native/v0.64.1/ios/hint && yarn hint:react-native:ios:lazy",
    "hint:react-native:ios:cli": "yarn workspace flow2hint cli --cwd ../.. --rootDir ./workbench/react-native/v0.64.1/ios/inputs",
    "hint:react-native:ios:cli-with-globs": "yarn hint:react-native:ios:cli \"./workbench/react-native/v0.64.1/ios/inputs/{index,Libraries/**/*}.js.flow\" \"!./workbench/react-native/v0.64.1/ios/inputs/Libraries/**/{__flowtests__,__mocks__,__tests__}/**/*.js.flow\" \"!./workbench/react-native/v0.64.1/ios/inputs/Libraries/{Inspector,JSInspector,NewAppScreen,ReactPrivate}/**/*.js.flow\" \"!./workbench/react-native/v0.64.1/ios/inputs/Libraries/Animated/AnimatedWeb.js.flow\" \"!./workbench/react-native/v0.64.1/ios/inputs/Libraries/{Promise,promiseRejectionIsError,promiseRejectionTrackingOptions}.js.flow\" \"!./workbench/react-native/v0.64.1/ios/inputs/Libraries/vendor/core/ErrorUtils.js.flow\"",
    "hint:react-native:ios:lazy": "yarn hint:react-native:ios:cli-with-globs --platform ios --outDir ./workbench/react-native/v0.64.1/ios/hint",
    "prettify": "prettier --write \"packages/**/*.ts\" && prettier --write \"packages/**/*.d.ts\" && prettier --write \"packages/**/*.js\"",
    "postinstall": "patch-package",
    "experiment:flowgetdef": "pushd \"./workbench/react-native/v0.64.1/ios/inputs\" && flow get-def ./index.js.flow 50 15 && popd"
  },
  "resolutions": {
    "@types/react": "16.9.56",
    "@types/react-native": "0.63.3"
  },
  "devDependencies": {
    "flow-bin": "0.137.0",
    "fs-extra": "^9.0.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  }
}